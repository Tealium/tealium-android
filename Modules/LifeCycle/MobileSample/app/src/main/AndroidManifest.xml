<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tealium.example"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.tealium.example.ExampleApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tealium.example.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tealium.example.ControlExpandableListActivity" >
        </activity>
        <activity
            android:name="com.tealium.example.ControlFragmentActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_custom_fragment" >
        </activity>
        <activity
            android:name="com.tealium.example.ControlGridViewActivity"
            android:label="@string/title_activity_control_grid_view" >
        </activity>
        <activity
            android:name="com.tealium.example.ControlStackViewActivity"
            android:label="@string/title_activity_control_stack_view" >
        </activity>
    </application>

</manifest>
